Total revenue
SELECT SUM(Price_USD * Sales_volume) AS total_revenue
FROM BMW_sales;

Total Unit Sold by model
SELECT model, SUM(Sales_volume) AS units_sold
FROM BMW_sales
GROUP BY model
ORDER BY units_sold DESC;

Sales by fuel type
SELECT fuel_type,
       SUM(Price_USD * Sales_volume) AS total_sales
FROM BMW_sales
GROUP BY fuel_type
ORDER BY total_sales DESC;

Average Selling Price
SELECT AVG(price_USD) AS avg_selling_price
FROM BMW_sales;

DISTINCT REGIONS
SELECT DISTINCT Region
FROM BMW_sales;












